##  [stringnode script: example 01: canon]

$tempo = 160;
[   `tempo $tempo ]
[   `frets 0 2 4 5 7 9 11 12]

for $x in 0...3 collect 
(
    [   `seq $x*9/8
        ##  instr_id    posns       str_offset      beat    numstr  bows    hopsize reps    gap
        [   [$x+1]      [2+($x%2)]  [$x%2]          1/16    9       2       0       8-$x    0 ] 
    ]   
)

##  quantization info
[   `quantization
    [ $tempo
        [6 8 1]
    ]
]
